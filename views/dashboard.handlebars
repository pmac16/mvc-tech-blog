<section>
    <h2>Your Posts</h2>
    {{#if posts.length}}
    <ol>
        {{#each posts as |post|}}
        <li>
            {{> post-info post}}
            <a href="/dashboard/edit/{{post.id}}" class="edit-link">Edit post</a>
        </li>
        {{/each}}
    </ol>
    {{else}}
      <p>You have no posts yet.</p>
    {{/if}}
</section>
<form action="/create">
  <button type="submit" class="btn">Create a New Post</button>
</form>





